    <script src="https://dev-econsultation.ecubix.com/assets/js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page
    <script src="https://dev-econsultation.ecubix.com/assets/js/dashboard.js"></script>-->
    <!-- End custom js for this page -->

